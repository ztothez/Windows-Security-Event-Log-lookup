{
  "manifest_version": 3,
  "name": "Windows Event Lookup",
  "description": "Lookup Windows Event IDs on Ultimate Windows Security",
  "version": "2.0",

  "icons": {
    "16": "images/icon16.png",
    "32": "images/icon32.png",
    "48": "images/icon48.png",
    "128": "images/icon128.png"
  },

  "background": {
    "scripts": ["background.js"]
  },

  "permissions": [
    "contextMenus",
    "tabs"
  ],

  "action": {
    "default_title": "Lookup Windows Event",
    "default_popup": "popup.html",
    "default_icon": {
      "16": "images/icon16.png",
      "32": "images/icon32.png",
      "48": "images/icon48.png",
      "128": "images/icon128.png"
    }
  },

  "browser_specific_settings": {
    "gecko": {
      "id": "{92128d6d-3abf-4da4-a5ce-59664aec9ed4}",
      "strict_min_version": "142.0",
      "data_collection_permissions": {
        "required": ["none"]
      }
    }
  }
}
